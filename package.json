{
  "name": "functional-models-orm",
  "version": "1.1.0",
  "description": "A basic ORM building library using the functional-models library.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "test:coverage": "nyc npm run test",
    "feature-tests": "./node_modules/.bin/cucumber-js -p default",
    "coverage": "nyc --all --reporter=lcov npm test",
    "dist": "tsc -p ./tsconfig.build.json && cp package.json ./dist && cp README.md ./dist && cd dist && npm publish"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monolithst/functional-models-orm.git"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/_tests_/**/*.*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "keywords": [
    "orm",
    "javascript",
    "nodejs"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Mike Cornwell",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/monolithst/functional-models-orm/issues"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.3.0",
    "@types/lodash": "^4.14.177",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.0",
    "cucumber": "^7.0.0-rc.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "homepage": "https://github.com/monolithst/functional-models-orm#readme",
  "dependencies": {
    "date-fns": "^2.25.0",
    "functional-models": "^1.1.20",
    "lodash": "^4.17.21"
  }
}
