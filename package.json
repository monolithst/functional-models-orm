{
  "name": "functional-models-orm",
  "version": "1.0.25",
  "description": "A basic ORM building library using the functional-models library.",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "npm run build && nyc mocha --recursive --full-trace --require source-map-support/register ./test/**/*.test.js",
    "feature-tests": "./node_modules/@cucumber/cucumber/bin/cucumber-js",
    "coverage": "npm run build && nyc --all --reporter=lcov npm test",
    "build": "rm -Rf ./dist && mkdir -p ./dist && cp -R ./src/* ./dist &&  cd ./dist && tsc -p ../tsconfig.json"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monolithst/functional-models-orm.git"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "dist/*.ts",
      "dist/*.js",
      "dist/**/*.ts",
      "dist/**/*.js"
    ],
    "cache": true,
    "extension": [
      ".ts"
    ],
    "require": [
      "source-map-support/register"
    ],
    "exclude": [
      "node_modules",
      "src/",
      "src/*.d.ts",
      "test/*"
    ],
    "instrument": true,
    "sourceMap": true
  },
  "keywords": [
    "orm",
    "javascript",
    "nodejs"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Mike Cornwell",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/monolithst/functional-models-orm/issues"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.7",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.0",
    "cucumber": "^7.0.0-rc.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.2",
    "source-map-support": "^0.5.21",
    "typescript": "^4.5.2"
  },
  "homepage": "https://github.com/monolithst/functional-models-orm#readme",
  "dependencies": {
    "date-fns": "^2.25.0",
    "functional-models": "^1.0.27",
    "lodash": "^4.17.21"
  }
}
